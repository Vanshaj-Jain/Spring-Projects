<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation="
 http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context-3.1.xsd">
 
  	<context:annotation-config />
	<bean class="com.springcore.autowire.anotation.Student" name="student" />
	<bean class="com.springcore.autowire.anotation.Address" name="address" p:name="Vanshaj" />
	<bean class="com.springcore.autowire.anotation.Address" name="address1" p:name="Vanshaj1" />
	
</beans>
